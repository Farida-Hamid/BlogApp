<div>
  <div class="user_container">
    <div class="photo"></div>
    <div class="user-info">
      <h1><%= @user.name%></h1>
      <h4>Number of posts: <%= @user.postscounter%></h4>
    </div>
  </div>
  <div class="bio">
    <h3>Bio</h3>
    <p><%= @user.bio%></p>
  </div>
  <div class="posts">
    <% @user.recent_posts.each_with_index do |post, index|%>
      <div  class="post">
      <h3>Post #<%= index+1%></h3>
        <p><%= post.text%></p>
        <p class="like-comment">Comments: <%= post.comments_counter%> Likes: <%= post.likes_counter%></p>
      </div>
    <%end%>
  </div>
  <button>See all post</button>
</div>